<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" verbose="4" thread-count="5"
	parallel="tests">

	<listeners>
		<listener
			class-name="com.aventstack.chaintest.plugins.ChainTestListener" />
		<listener
			class-name="com.qa.opencart.listeners.TestAllureListener" />
		<listener
			class-name="com.qa.opencart.listeners.AnnotationTransformer" />
	</listeners>


	<test name="Login Page Test_firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.qa.opencart.test.LoginPageTest" />
		</classes>
	</test>


	
</suite>	